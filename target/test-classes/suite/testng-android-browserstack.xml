<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Android BrowserStack Parallel Suite"
       parallel="methods"
       thread-count="5">

    <listeners>
        <listener class-name="listeners.DeviceExecutionSummaryListener"/>
    </listeners>

    <test name="Android Scenario Parallel Execution">
        <classes>
            <class name="runners.TestRunner"/>
        </classes>
    </test>

</suite>
